<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nurika.motionlayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Simple1Activity" />
        <activity android:name=".Simple2Activity" />
        <activity android:name=".SimpleTouchUpActivity" />
        <activity android:name=".ImageFilterActivity" />
        <activity android:name=".KeyPosition1Activity" />
        <activity android:name=".KeyPosition2Activity" />
        <activity android:name=".KeyPosition3Activity" />
        <activity android:name=".CooLayoutActivity" />
        <activity android:name=".ParallaxActivity" />
        <activity android:name=".ViewPagerActivity" />
    </application>

</manifest>